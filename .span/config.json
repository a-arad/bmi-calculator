{
  "orchestration": {
    "auto_retry": true,
    "default_model": "anthropic/claude-3.5-sonnet",
    "max_parallel_tasks": 3
  },
  "project": {
    "architecture": {
      "backend": {
        "framework": "Express",
        "language": "Node.js",
        "libraries": [
          "cors",
          "body-parser"
        ]
      },
      "database": null,
      "deployment": "Vercel",
      "frontend": {
        "framework": "React",
        "language": "JavaScript",
        "libraries": [
          "styled-components"
        ]
      }
    },
    "description": "A simple BMI (Body Mass Index) calculator web application",
    "name": "bmi-calculator"
  },
  "version": "1.0",
  "workflows": {
    "claude_workflow": ".github/workflows/claude.yml",
    "span_orchestrator": ".github/workflows/span-orchestrator.yml"
  }
}